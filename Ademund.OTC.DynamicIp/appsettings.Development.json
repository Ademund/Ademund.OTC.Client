{
  "DynamicIp": {
    "AutoStart": true,
    "StartMinimized": true,
    "IntervalInMinutes": 15,
    "ShowChangeNotifications": true,
    "ProxyAddress": "127.0.0.1:8888",
    "UseProxy": true,
    "Environments": [
      {
        "Name": "Env1",
        "Region": "eu-de"
      },
      {
        "Name": "Env2",
        "Region": "eu-de"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 1000000,
          "retainedFileCountLimit": 7
        }
      }
    ]
  }
}